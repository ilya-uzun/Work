VAR
	COM_SERVICE1: COM_SERVICE;	   (*Г”ГЃ Г®ГІГЄГ°Г»ГІГЁГї ГЇГ®Г°ГІГ *)
	port_opened:  BYTE := 0;				(*Г±Г®Г±ГІГ®ГїГ­ГЁГҐ ГЇГ®Г°ГІГ *)
