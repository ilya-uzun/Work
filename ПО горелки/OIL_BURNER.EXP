FUNCTION_BLOCK DriverBoiler
VAR_INPUT
	StartIn:BOOL;
	RegStart:BOOL;
	PV:REAL;
	Beat:WORD;
	SP:REAL;
	DB:REAL;
	Xp:REAL;
	Xi:REAL;
	Xd:REAL;
	Off:INT;
END_VAR
VAR_OUTPUT
	StartOut:BOOL;
	Y:INT;
END_VAR
VAR


	Kp:REAL;
	Ki:REAL;
	Kd:REAL;
	EiOld:REAL;
	BeatOld:WORD;
	ton1:INT;

END_VAR
(* @END_DECLARATION := '0' *)
(*Г”Г®Г°Г¬ГіГ«Г  ГЏГ€Г„ Г°ГҐГЈГіГ«ГЁГ°Г®ГўГ Г­ГЁГї:
	Y	=	Xp*Ei	+	Xi*summ(Ei)	+	Xd*dEi,
ГЈГ¤ГҐ
	Ei			-	Г°Г Г±Г±Г®ГЈГ«Г Г±Г®ГўГ Г­ГЁГҐ Г¬ГҐГ¦Г¤Гі ГІГҐГЄГіГ№ГЁГ¬ Г§Г­Г Г·ГҐГ­ГЁГҐГ¬ ГЁ ГіГ±ГІГ ГўГЄГ®Г©
	Summ		-	Г­Г ГЄГ®ГЇГ«ГҐГ­Г­Г Гї Г®ГёГЁГЎГЄГ  Г°Г Г±Г±Г®ГЈГ«Г Г±Г®ГўГ Г­ГЁГ©
	dEi			-	Г°Г Г§Г­ГЁГ¶Г  Г¬ГҐГ¦Г¤Гі Г¤ГўГіГ¬Гї Г±Г®Г±ГҐГ¤Г­ГЁГ¬ГЁ Г°Г Г±Г±Г®ГЈГ«Г Г±Г®ГўГ Г­ГЁГїГ¬ГЁ
	Xp			-	ГЇГ°Г®ГЇГ®Г°Г¶ГЁГ®Г­Г Г«ГјГ­Г»Г© ГЄГ®ГЅГґГґГЁГ¶ГЁГҐГ­ГІ
	Xi			-	ГЁГ­ГІГҐГЈГ°Г Г«ГјГ­Г»Г© ГЄГ®ГЅГґГґГЁГ¶ГЁГҐГ­ГІ
	Xd			-	Г¤ГЁГґГґГҐГ°ГҐГ­Г¶ГЁГ Г«ГјГ­Г»Г© ГЄГ®ГЅГґГґГЁГ¶ГЁГҐГ­ГІ
	Y			-	ГўГ»ГµГ®Г¤.
*)

IF StartIn THEN
	IF Beat<>BeatOld THEN
		IF SP-PV>DB AND NOT StartOut THEN StartOut:=TRUE; END_IF;
		IF RegStart THEN
			IF ABS(SP-PV)>DB THEN

				Kp:=Xp*(SP-PV);

				Ki:=Ki+Xi*(SP-PV);
				IF Ki<0.0 THEN Ki:=0.0; ELSIF Ki>100.0 THEN Ki:=100.0; END_IF;

				Kd:=Xd*(SP-PV-EiOld); EiOld:=SP-PV;

				Y:=REAL_TO_INT(Kp+Ki+kd);
				IF Y<0 THEN Y:=0; ELSIF Y>100 THEN Y:=100; END_IF;
			END_IF;

		ELSE
			Ki:=0.0; EiOld:=0.0; Y:=0;
		END_IF;
	END_IF;

	IF PV>Off AND RegStart THEN
		IF ton1<50 THEN
			ton1:=ton1+1;
		ELSE
			StartOut:=FALSE;
			ton1:=0;
		END_IF;
	ELSE ton1:=0;
	END_IF;
	BeatOld:=Beat;
ELSE StartOut:=FALSE;
END_IF;


END_FUNCTION_BLOCK


